{
  "name": "@time.matters/rest",
  "sideEffects": false,
  "private": false,
  "version": "0.0.3",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "buildDev": "rollup --config ../../config/rollup.lib.mjs --environment MODE:development,ENTRY:index.ts",
    "build": "rollup --config ../../config/rollup.lib.mjs --environment MODE:production,ENTRY:index.ts",
    "watch": "rollup --watch --config ../../config/rollup.lib.mjs --environment MODE:development,ENTRY:index.ts,WATCH:true"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.11",
    "nodemon": "^3.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "files": [
    "package.json",
    "tsconfig.json",
    "dist/**/*.d.ts",
    "dist/*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}
